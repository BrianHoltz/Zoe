   Me.Cycle,
   Modulus 3000,
   Not
} Do {
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn
}
