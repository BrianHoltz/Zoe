When {
   Me.SeeSomething And Not It.IsSameSpecies And It.IsAlive
} Do {
   Turn Me.Away,
   Move
}

When {
   Me.FeelSomething And Not It.IsAlive
} Do {
   Bite
}

When {
   Me.SeeSomething And Not It.IsAlive
} Do {
   Turn Me.Toward,
   Move
}

When {
   Me.Cycle,
   Modulus 3000,
   Not
} Do {
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn,
   Spawn
}
